{
	"name": "mariage-serein-api",
	"version": "0.0.1",
	"description": "api pour transfonum web",
	"author": "transfonum",
	"bin": "dist/main.js",
	"main": "dist/main.js",
	"engines": {
		"node": ">=10.0.0",
		"npm": "~6.10.0"
	},
	"license": "MIT",
	"scripts": {
		"migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
		"migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n migration",
		"schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
		"build": "rimraf dist && tsc -p tsconfig.build.json",
		"gcp-build": "npm run build",
		"deploy": "gcloud app deploy",
		"format": "prettier --write \"src/**/*.ts\"",
		"start": "node ./dist/main.js",
		"start:local": "cross-env NODE_ENV=local ts-node -r tsconfig-paths/register src/main.ts",
		"start:onpremise": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/main.ts",
		"start:dev": "cross-env NODE_ENV=development tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
		"start:debug": "cross-env NODE_ENV=development tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
		"start:prod": "cross-env NODE_ENV=production node dist/src/main.js",
		"lint": "tslint -p tsconfig.json -c tslint.json",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"create-env": "printenv > gcp.env",
		"pretypeorm": "(rm ormconfig.json || :) && ts-node -r tsconfig-paths/register src/scripts/write-type-orm-config.ts",
		"typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
		"typeorm:migration:generate": "npm run typeorm -- migration:generate -n",
		"typeorm:migration:run": "npm run typeorm -- migration:run"
	},
	"dependencies": {
		"@google-cloud/logging-bunyan": "^2.0.0",
		"@google-cloud/storage": "^4.1.0",
		"@iaminfinity/nodemailer": "^0.1.5",
		"@nest-modules/mailer": "^1.1.3",
		"@nestjs/common": "^6.0.0",
		"@nestjs/core": "^6.0.0",
		"@nestjs/passport": "^6.1.0",
		"@nestjs/platform-express": "^6.0.0",
		"@nestjs/platform-socket.io": "^7.0.7",
		"@nestjs/schedule": "^0.3.0",
		"@nestjs/swagger": "^4.1.14",
		"@nestjs/typeorm": "^6.1.3",
		"@types/elasticsearch": "^5.0.34",
		"@types/lodash": "^4.14.149",
		"@types/node-fetch": "^2.5.3",
		"@types/uuid": "^3.4.5",
		"auth0": "^2.20.0",
		"axios": "^0.19.2",
		"body-parser": "^1.19.0",
		"bunyan": "^1.8.12",
		"class-transformer": "^0.2.3",
		"class-validator": "^0.11.0",
		"cross-env": "^7.0.0",
		"date-fns": "^2.11.0",
		"dotenv": "^8.2.0",
		"elastic-apm-node": "^3.3.0",
		"elasticsearch": "^16.6.0",
		"express-jwt": "^5.3.1",
		"fs": "0.0.1-security",
		"jwks-rsa": "^1.6.2",
		"lodash": "^4.17.15",
		"multer": "^1.4.2",
		"multer-google-storage": "^1.3.0",
		"nest-status-monitor": "^0.1.3",
		"nestjs-apm-v6": "^1.0.1",
		"nestjs-typeorm-paginate": "^1.0.3",
		"node-fetch": "^2.6.0",
		"node-mailjet": "^3.3.1",
		"passport": "^0.4.1",
		"passport-jwt": "^4.0.0",
		"path": "^0.12.7",
		"pdfkit": "^0.11.0",
		"pdfmake": "^0.1.63",
		"pg": "^7.12.0",
		"reflect-metadata": "^0.1.12",
		"rimraf": "^2.6.2",
		"rxjs": "^6.3.3",
		"stripe": "^7.8.0",
		"swagger-ui-express": "^4.1.3",
		"twilio": "^3.39.4",
		"typeorm": "^0.2.22",
		"uuid": "^3.4.0"
	},
	"devDependencies": {
		"@nestjs/testing": "^6.11.11",
		"@types/auth0": "^2.20.0",
		"@types/dotenv": "^8.2.0",
		"@types/express": "4.17.2",
		"@types/jest": "25.1.2",
		"@types/node": "13.7.0",
		"@types/passport-jwt": "^3.0.3",
		"@types/stripe": "^7.13.21",
		"@types/supertest": "2.0.8",
		"jest": "25.1.0",
		"prettier": "1.19.1",
		"supertest": "4.0.2",
		"ts-jest": "25.2.0",
		"ts-node": "8.6.2",
		"tsc-watch": "4.1.0",
		"tsconfig-paths": "3.9.0",
		"tslint": "^6.1.1",
		"typescript": "3.7.5"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
