    "tslib": "^1.10.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^8.3.2",
    "@angular-devkit/build-angular": "^0.901.8",
    "@angular/cli": "^9.1.8",
    "@angular/compiler-cli": "^9.1.11",
    "@angular/language-service": "~9.1.11",
    "@angularclass/hmr": "^2.1.3",
    "@nguniversal/builders": "^9.1.1",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.2",
    "@types/leaflet": "^1.5.8",
    "@types/node": "~13.7.0",
    "codelyzer": "^5.2.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-loader": "^6.2.1",
    "ts-node": "~8.6.2",
    "tslint": "~6.0.0",
    "typescript": "~3.7.5",
    "webpack": "latest",
    "webpack-cli": "^3.3.10"
  }
}
