{
  "name": "mariage-serein-web",
  "version": "0.0.1-dev",
  "bin": "dist/server.js",
  "main": "server.ts",
  "engines": {
    "node": "~10.0.0",
    "npm": "~6.10.0"
  },
  "scripts": {
    "postinstall": "ngcc",
    "hmr": "ng serve --configuration hmr --live-reload true",
    "format": "prettier --write \"projects/**/*.{ts,js}\"",
    "ng": "ng",
    "start:local": "ng serve --host=0.0.0.0 --disableHostCheck",
    "start": "node dist/server/main.js",
    "build": "ng build",
    "test": "ng test --detectOpenHandles --color",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "test:watch": "npm run test --watch",
    "test:ci": "npm run test:coverage --ci --runInBand",
    "test:coverage": "npm test --coverage",
    "start:ci": "node dist/server.js",
    "build:ssr-gcp": "npm run build:client-and-server-bundles-gcp && npm run compile:server_bak",
    "build:client-and-server-bundles-gcp": "npm run oldspacesize && node_modules/@angular/cli/bin/ng build --configuration=gcp && node_modules/@angular/cli/bin/ng run mariage:server:gcp --bundleDependencies all",
    "oldspacesize": "node --max-old-space-size=1457 ",
    "compile:server_bak": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr_bak": "npm run build:client-and-server-bundles && npm run compile:server",
    "serve:ssr_bak": "npm run build:ssr && node dist/server",
    "build:client-and-server-bundles_bak": "ng build --prod && ng run mariage:server:production --bundleDependencies all",
    "dev:ssr": "ng run mariage:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run mariage:server:production",
    "prerender": "ng run mariage:prerender"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@agm/snazzy-info-window": "^1.1.0",
    "@angular/animations": "~9.1.11",
    "@angular/cdk": "~9.2.4",
    "@angular/common": "~9.1.11",
    "@angular/compiler": "~9.1.11",
    "@angular/core": "~11.0.5",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "~9.1.11",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.11",
    "@angular/platform-browser-dynamic": "~9.1.11",
    "@angular/platform-server": "~9.1.11",
    "@angular/pwa": "^0.900.1",
    "@angular/router": "~9.1.11",
    "@angular/service-worker": "~9.1.11",
    "@asymmetrik/ngx-leaflet": "^6.0.1",
    "@auth0/angular-jwt": "^4.0.0",
    "@auth0/auth0-spa-js": "^1.6.3",
    "@codewithdan/observable-store": "^2.2.4",
    "@ionic/angular": "^4.11.10",
    "@ng-select/ng-select": "^3.7.2",
    "@nguniversal/express-engine": "^9.1.1",
    "@ngx-loading-bar/core": "^4.2.0",
    "@ngx-loading-bar/http-client": "^4.2.0",
    "@ngx-loading-bar/router": "^4.2.0",
    "@ngx-translate/core": "^12.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/date-fns": "^2.6.0",
    "@types/googlemaps": "^3.39.2",
    "@uiowa/digit-only": "^1.3.0",
    "angular-iban": "^0.2.0",
    "auth0-js": "^9.12.2",
    "browser-image-compression": "^1.0.6",
    "core-js": "^3.6.4",
    "date-fns": "^2.11.0",
    "express": "^4.17.1",
    "geokit": "^0.1.4",
    "hammerjs": "^2.0.8",
    "iban": "0.0.14",
    "leaflet": "^1.6.0",
    "moment": "^2.26.0",
    "ng5-slider": "^1.2.4",
    "ngx-avatar": "^3.7.0",
    "ngx-custom-validators": "^8.0.0",
    "ngx-daterangepicker-material": "^2.1.11",
    "ngx-drag-scroll": "^8.0.0-beta.2",
    "ngx-markdown": "^9.1.1",
    "ngx-mat-select-search": "^2.1.1",
    "ngx-quicklink": "^0.2.0",
    "ngx-stars": "^1.4.3",
    "ngx-toastr": "^12.0.0",
    "npm-run-all": "^4.1.5",
